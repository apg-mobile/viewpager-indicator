<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ViewPagerIndicator">
        <attr name="indicator_color" format="color"/>
        <attr name="indicator_selectedColor" format="color"/>
        <attr name="indicator_sizeOffSet" format="dimension"/>
        <attr name="indicator_items" format="integer"/>
        <attr name="indicator_stokeWidth" format="dimension"/>
        <attr name="indicator_spacing" format="dimension"/>
        <attr name="indicator_startPosition" format="integer"/>
        <attr name="indicator_allFill" format="boolean"/>
        <attr name="indicator_gravity" format="enum">
            <enum name="center" value="0"/>
            <enum name="left" value="-1"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
</resources>